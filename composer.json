{
  "name": "druix/expo-server-sdk-php",
  "description": "Server-side library for working with the Expo push notification service using PHP 8.4+",
  "keywords": ["expo", "push", "php", "expo-server-sdk-php"],
  "version": "0.1.0",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/dru1x/expo-server-sdk-php",
  "support": {
    "issues": "https://github.com/dru1x/expo-server-sdk-php/issues",
    "source": "https://github.com/dru1x/expo-server-sdk-php"
  },
  "authors": [
    {
      "name": "James Drew",
      "email": "james.mcw.drew@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.4",
    "ext-json": "*",
    "ext-zlib": "*",
    "saloonphp/saloon": "^3.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox",
    "test-coverage": "@test --coverage-html coverage"
  },
  "autoload": {
    "psr-4": {
      "Dru1x\\ExpoPush\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dru1x\\ExpoPush\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  }
}
